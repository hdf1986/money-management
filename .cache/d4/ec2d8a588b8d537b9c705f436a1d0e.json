s","original":{"line":33,"column":15}},{"generated":{"line":71,"column":4},"source":"components/TransactionList.js","original":{"line":32,"column":12}},{"generated":{"line":71,"column":5},"source":"components/TransactionList.js","original":{"line":26,"column":4}},{"generated":{"line":71,"column":6},"source":"components/TransactionList.js","original":{"line":25,"column":2}},{"generated":{"line":72,"column":0},"source":"components/TransactionList.js","original":{"line":55,"column":1}},{"generated":{"line":72,"column":1},"source":"components/TransactionList.js","original":{"line":13,"column":0}},{"name":"TransactionList","generated":{"line":74,"column":15},"source":"components/TransactionList.js","original":{"line":57,"column":15}}],"sources":{"components/TransactionList.js":"import React, { useEffect, useState} from 'react'\nimport Container from 'react-bootstrap/Container';\nimport Table from 'react-bootstrap/Table';\nimport Spinner from 'react-bootstrap/Spinner';\nimport Accordion from 'react-bootstrap/Accordion';\nimport Card from 'react-bootstrap/Card';\nimport Button from 'react-bootstrap/Button';\nimport ListGroup from 'react-bootstrap/ListGroup';\n\nimport formatMoney from '../utils/formatMoney';\nimport capitalizeFirstLetter from '../utils/capitalizeFirstLetter';\n\nconst TransactionList = () => {\n  const [transactions, setTransactions] = useState(null)\n  useEffect(() => {\n    fetch('/api/v1/transactions')\n      .then(res => res.json())\n      .then(body => setTransactions(body))\n  }, [])\n  if(transactions === null) return (\n    <Spinner animation=\"grow\" role=\"status\">\n      <span className=\"sr-only\">Loading transactions...</span>\n    </Spinner>\n  )\n  return (\n    <Container>\n      <h1>Transactions history</h1>\n      {\n        transactions.length === 0 ?\n          'No transactions' :\n          (\n            <Accordion>\n              {transactions.map((transaction, index) => (\n                <Card key={transaction.id}>\n                  <Card.Header>\n                    <Accordion.Toggle as={Button} variant=\"text\" eventKey={transaction.id}>\n                      {capitalizeFirstLetter(transaction.type)} - {formatMoney(transaction.amount)}\n                    </Accordion.Toggle>\n                  </Card.Header>\n                  <Accordion.Collapse eventKey={transaction.id}>\n                    <Card.Body>\n                      <ListGroup>\n                        <ListGroup.Item><strong>ID:</strong> {transaction.id}</ListGroup.Item>\n                        <ListGroup.Item><strong>Date:</strong> {new Date(transaction.effectiveDate).toLocaleDateString()}</ListGroup.Item>\n                      </ListGroup>\n                    </Card.Body>\n                  </Accordion.Collapse>\n                </Card>\n              ))}\n            </Accordion>\n          )\n      }\n    </Container>\n  )\n}\n\nexport default TransactionList\n"},"lineCount":null}},"error":null,"hash":"db4ea606ef43386baa52cf3b90933a6f","cacheData":{"env":{}}}